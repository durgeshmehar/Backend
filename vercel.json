{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/assets"
      }
    ],
    "routes": [
      {
        "src": "/products",
        "dest": "index.js"
      },
      {
        "src": "/products/.*",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "build/index.html"
      },
      {
        "src": "/assets/(.+)",
        "dest": "build/assets/$1"
      },
      {
        "src": "/.+",
        "dest": "build/$1"
      }
    ]
 
  }