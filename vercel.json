{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "API_KEY": "your-api-key"
    }
  }